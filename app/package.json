{
  "name": "Eth App",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "15.x"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.10",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "24.0.17",
    "@types/node": "^12.7.1",
    "@types/query-string": "^6.3.0",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-app": "^10.0.0",
    "camelcase": "^6.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "core-js": "^3.10.1",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "domain-browser": "^4.19.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "extended-define-webpack-plugin": "^0.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "is-wsl": "^2.2.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.2",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.2.1",
    "react-dev-utils": "^11.0.4",
    "replace-in-file": "^6.2.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-pnp": "1.1.2",
    "typescript": "^3.8.1",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.1.1",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-auth-refresh": "^2.2.8",
    "axios-retry": "^3.1.8",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^8.1.0",
    "immer": "^9.0.1",
    "lodash": "^4.17.21",
    "object-assign": "^4.1.1",
    "promise": "^8.1.0",
    "query-params": "^0.0.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "semver": "^5.7.0",
    "styled-components": "^5.3.0",
    "styled-components-grid": "^2.2.2",
    "webpack": "^5.34.0"
  },
  "scripts": {
    "postinstall": "node postinstall/index.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./src -f stylish --ext .ts --ext .tsx",
    "lint:fix": "prettier --write \"src/**/**/*.ts*\" && eslint --fix \"src/**/**/*.tsx*\"",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globals": {
      "APP_CONFIG": {
        "api": {
          "url": "https://localhost:5000"
        }
      }
    },
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": false
        }
      ]
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  }
}
